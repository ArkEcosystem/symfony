{
    "name": "arkecosystem/bundle",
    "description": "A Symfony bundle for Ark Core.",
    "keywords": ["symfony", "bundle", "ark", "blockchain", "crypto", "currency"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "brian@ark.io",
        "homepage": "https://github.com/faustbrian"
    }],
    "require": {
        "php": "^7.1",
        "arkecosystem/client": "^1.0.0",
        "symfony/dependency-injection": "^5.0.0",
        "symfony/framework-bundle": "^5.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "psr-4": {
            "ArkEcosystem\\ArkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArkEcosystem\\ArkBundle\\Tests\\": "Tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
